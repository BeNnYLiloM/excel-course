{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parens": "off",
    "indent": [2],
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "operator-linebreak": "off",
    "max-len": "off"
  },
  "plugins": [
    "jest"
  ],
  "extends": ["eslint:recommended", "google"]
}
